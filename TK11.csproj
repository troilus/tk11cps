<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <AssemblyName>TK11</AssemblyName>
    <GenerateAssemblyInfo>False</GenerateAssemblyInfo>
    <OutputType>WinExe</OutputType>
    <UseWindowsForms>True</UseWindowsForms>
    <TargetFramework>net45</TargetFramework>
    <Prefer32Bit>True</Prefer32Bit>
  </PropertyGroup>
  <PropertyGroup>
    <LangVersion>11.0</LangVersion>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup>
    <ApplicationIcon>app.ico</ApplicationIcon>
    <ApplicationManifest>app.manifest</ApplicationManifest>
    <RootNamespace />
  </PropertyGroup>
  <ItemGroup />
  <ItemGroup>
    <Reference Include="System.Core" />
    <Reference Include="NAudio">
      <HintPath>..\..\..\..\Program Files (x86)\TK11\NAudio.dll</HintPath>
    </Reference>
    <Reference Include="CsvHelper">
      <HintPath>..\..\..\..\Program Files (x86)\TK11\CsvHelper.dll</HintPath>
    </Reference>
    <Reference Include="Ude">
      <HintPath>..\..\..\..\Program Files (x86)\TK11\Ude.dll</HintPath>
    </Reference>
  </ItemGroup>
</Project>